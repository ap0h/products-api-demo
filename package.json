{
  "name": "graphql",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon server.js",
    "test:unit": "jest --detectOpenHandles --forceExit --coverage",
    "typecheck": "tsc --noEmit",
    "start": "dotenv -e ./.env -- ts-node src",
    "format": "xo --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "xo": {
    "prettier": true,
    "semicolon": false,
    "rules": {
      "unicorn/prefer-module": "off",
      "import/extensions": "off",
      "no-template-curly-in-string": "off",
      "n/prefer-global/process": "off",
      "unicorn/no-array-reduce": "off",
      "import/no-anonymous-default-export": "off",
      "n/prefer-global/buffer": "off",
      "@typescript-eslint/naming-convention": "off",
      "@typescript-eslint/consistent-type-definitions": [
        "error",
        "interface"
      ],
      "@typescript-eslint/ban-types": [
        "error",
        {
          "types": {
            "null": false
          },
          "extendDefaults": true
        }
      ]
    }
  },
  "dependencies": {
    "axios": "^1.6.2",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-graphql": "^0.12.0",
    "fast-csv": "^4.3.6",
    "graphql": "^16.8.1",
    "helmet": "^7.1.0",
    "mongoose": "^8.0.3"
  },
  "devDependencies": {
    "@swc/core": "^1.3.101",
    "@swc/jest": "^0.2.29",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "dotenv-cli": "^7.3.0",
    "jest": "^29.7.0",
    "nodemon": "^2.0.15",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "xo": "^0.56.0"
  }
}
