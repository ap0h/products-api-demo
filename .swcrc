{
    "$schema": "https://json.schemastore.org/swcrc",
    "module": {
        "strict": true,
        "type": "commonjs"        
    },
    "jsc": {
        "baseUrl": ".",
        "paths": {
          "@config/*": ["src/config/*"],
          "@core/*": ["src/core/*"],
          "@libs/*": ["src/libs/*"],
          "@repositories/*": ["src/repositories/*"],
          "@use-cases/*": ["src/use-cases/*"],
          "@services/*": ["src/services/*"]
        },
        "parser": {
        "syntax": "typescript",
        "tsx": false,
        "dynamicImport": false,
        "decorators": false
        },
        "transform": {},
        "target": "es5",
        "loose": true,
        "externalHelpers": false,
        // Requires v1.2.50 or upper and requires target to be es2016 or upper.
        "keepClassNames": true
    },
    "minify": false
   }
   